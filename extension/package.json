{
  "name": "ai-job-assistant-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@geist-ui/core": "^2.3.8",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-spring/web": "^9.7.2",
    "@supabase/supabase-js": "^2.38.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.292.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-use-gesture": "^9.1.3",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run build:extension",
    "build:extension": "cp manifest.chrome.json out/manifest.json && cp -r scripts out/ && npm run fix-popup && node scripts/fixPopupPaths.js",
    "build:firefox": "next build && cp manifest.firefox.json out/manifest.json && cp -r scripts out/ && npm run fix-popup && node scripts/fixPopupPaths.js && node scripts/updateCspHashes.js",
    "fix-popup": "cp out/index.html out/popup.html",
    "start": "next start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/node": "^16.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17"
  }
}
